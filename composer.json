{
    "name": "kanata-php/conveyor-laravel-broadcaster",
    "description": "Conveyor Laravel Broadcaster",
    "autoload": {
        "psr-4": {
            "Kanata\\LaravelBroadcaster\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Savio Resende"
        }
    ],
    "require": {
        "firebase/php-jwt": "^6.4"
    },
    "require-dev": {
        "phpro/grumphp": "*",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.41"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kanata\\LaravelBroadcaster\\ConveyorServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
